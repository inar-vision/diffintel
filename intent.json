{
  "version": "0.1",
  "features": [
    {
      "id": "list-users",
      "type": "http-route",
      "method": "GET",
      "path": "/users"
    },
    {
      "id": "get-user",
      "type": "http-route",
      "method": "GET",
      "path": "/users/:id"
    },
    {
      "id": "create-user",
      "type": "http-route",
      "method": "POST",
      "path": "/users"
    },
    {
      "id": "delete-user",
      "type": "http-route",
      "method": "DELETE",
      "path": "/users/:id"
    },
    {
      "id": "health-check",
      "type": "http-route",
      "method": "GET",
      "path": "/health"
    },
    {
      "id": "update-user",
      "type": "http-route",
      "method": "PUT",
      "path": "/users/:id"
    },
    {
      "id": "search-users",
      "type": "http-route",
      "method": "GET",
      "path": "/users/search"
    }
  ]
}
